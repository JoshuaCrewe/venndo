{
  "name": "venndo",
  "description": "A tool for doing mundane or repetitive tasks.",
  "version": "0.1.0",
  "author": "Joshua Crewe",
  "bin": {
    "venndo": "./bin/run"
  },
  "bugs": "https://github.com/JoshuaCrewe/venndo/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "ansi-colors": "^3.2.3",
    "conf": "^2.2.0",
    "execa": "^1.0.0",
    "fancy-log": "^1.3.3",
    "gift": "^0.10.2",
    "inquirer": "^6.2.2",
    "listr": "^0.14.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/conf": "^2.1.0",
    "@types/execa": "^0.9.0",
    "@types/fancy-log": "^1.3.0",
    "@types/fs-extra": "^5.0.4",
    "@types/inquirer": "^0.0.43",
    "@types/listr": "^0.13.0",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/JoshuaCrewe/venndo",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "venndo",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "JoshuaCrewe/venndo",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
