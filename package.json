  "author": "Joshua Crewe",
  "bin": {
    "venndo": "./bin/run"
  },
  "bugs": "https://github.com/JoshuaCrewe/venndo/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3",
    "@types/del": "^4.0.0",
    "ansi-colors": "^4.1.1",
    "conf": "^6.2.0",
    "del": "^5.1.0",
    "execa": "^4.0.0",
    "fancy-log": "^1.3.3",
    "inquirer": "^7.0.1",
    "listr": "^0.14.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4.2.7",
    "@types/conf": "^3.0.0",
    "@types/execa": "^2.0.0",
    "@types/fancy-log": "^1.3.0",
    "@types/fs-extra": "^8.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/listr": "^0.14.2",
    "@types/mocha": "^5",
    "chai": "^4",
    "globby": "^10.0.1",
    "mocha": "^6.2.2",
    "nyc": "^15.0.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/JoshuaCrewe/venndo",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "venndo",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "JoshuaCrewe/venndo",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
